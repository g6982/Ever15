const { Component } = owl;

const Tooltip = __exports.Tooltip = class Tooltip extends Component {}
Tooltip.template = "web.Tooltip";
Tooltip.props = {
    tooltip: { type: String, optional: true },
    template: { type: String, optional: true },
    info: { optional: true },
};

return __exports;
});
