/*************************************************************
*  Filepath: /web/static/src/legacy/js/public/public_env.js  *
*  Bundle: web.assets_frontend_lazy                          *
*  Lines: 11                                                 *
*************************************************************/
odoo.define("web.public_env", function (require) {
