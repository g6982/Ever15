const elementsWithoutDataHotkey=getVisibleElements(ui.activeElement,"[accesskey]:not([data-hotkey])");for(const el of elementsWithoutDataHotkey){el.dataset.hotkey=el.accessKey;el.removeAttribute("accesskey");}
