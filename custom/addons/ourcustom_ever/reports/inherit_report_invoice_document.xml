<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_invoice_inherit_ever" inherit_id="account.report_invoice">
        <!--Invoice language will be based on site/instance language-->
        <t t-set="lang" t-value="o.invoice_user_id.sudo().lang if o.move_type in ('in_invoice', 'in_refund') else o.partner_id.lang" position="replace">
           <t t-set="lang" t-value="o.invoice_user_id.sudo().lang if o.move_type in ('in_invoice', 'in_refund') else o.woo_lang_id.code"/>
        </t>
    </template>

    <!-- inherit account.report_invoice_document to show the right template in invoice preview-->
    <template id="report_invoice_document_preview" inherit_id="account.report_invoice_document">
         <xpath expr="//div[@class='page']" position="replace">
             <!--Replace by content of div with calass=page from template report_to_invoice_qr_fin -->
             <div class="page" name="all_pages" style="">
                <div class="page" name="first_invoice_qr">
                    <div name="address" style="width:80mm;height:45mm;">
                        <!--<t t-esc="address"/>-->
                        <p style="color:#38393b;text-decoration:underline;font-size:11px">Fulfillment.swiss, Eversmart, Isenrietstr.19, 8617 Mönchaltorf</p>
                        <span t-field="o.partner_id.name"/><br/>
                        <span t-field="o.partner_id.street"/>&#032;<span t-field="o.partner_id.street2"/><br/>
                        <span t-field="o.partner_id.zip"/>&#032;<span t-field="o.partner_id.city"/>&#032;<span t-field="o.partner_id.state_id"/><br/>
                        <span t-field="o.partner_id.country_id"></span>
                    </div>

                    <h2 style="margin-top:9px">
                        <strong><span style="font-size:18px;">Rechnung</span></strong>
                        <strong><span style="font-size:18px" t-if="o.name != '/'" t-field="o.name"/></strong>
                    </h2>

                    <style>
                        .text_capitalize_mod::first-letter{
                              text-transform: capitalize;
                        }

                        .myDiv {
                            display: inline-block;
                            font-weight:900;
                            }
                        .carret{
                            display:inline-block;
                            border:1px solid #595958;
                            width:12px;
                            height:12px;
                            }
                     </style>

                    <!--Add borders to these informations bellow-->
                    <table style="width:100%;border: 1px solid black;border-left:none;border-right:none">
                        <tr style="border: 1px solid black;border-left:none;border-right:none">
                            <td colspan="4" style="vertical-align:middle;">
                                <strong><span class="myDiv">Ihre Bestellung bei &#160;</span><span class="text_capitalize_mod myDiv"  t-field="o.woo_instance_id.sudo().name"/>&#160;<span t-field="o.ref"/></strong>
                            </td>
                        </tr>
                        <tr style="border: 1px solid black;border-left:none;border-right:none;border-bottom:none">
                            <td style="border-bottom:none;vertical-align:top;font-size:15px">
                                <p name="invoice_date">Datum:</p>
                                <p name="customer_code" style="margin-top:-15px;margin-bottom:0px">Kundennummer:</p>
                            </td>
                            <td style="border-bottom:none;vertical-align:top;font-size:15px">
                                <p t-field="o.invoice_date" t-options='{"format": "dd.MM.yyyy"}'/>
                                <p style="margin-top:-15px;margin-bottom:0px" t-field="o.partner_id.ref"/>
                            </td>
                            <td style="border-bottom:none;vertical-align:top;font-size:15px">
                                <p name="due_date">Zahlbar bis:</p>
                            </td>
                            <!-- If The invoice is Paid show 'Paid' else show date-->
                            <t t-if="o.payment_state == 'paid'">
                                <td style="border-bottom:none;vertical-align:top;font-size:15px">
                                    <p>Bezahlt</p>
                                </td>
                            </t>
                            <t t-else="">
                                <td style="border-bottom:none;vertical-align:top;font-size:15px">
                                    <p t-field="o.invoice_date_due" t-options='{"format": "dd.MM.yyyy"}'/>
                                </td>
                            </t>
                        </tr>
                    </table>

                        <!-- TABLE PRINCIPALE -->
                    <t t-set="display_discount" t-value="any(l.discount for l in o.invoice_line_ids)"/>
                    <table class="table table-sm o_main_table" name="invoice_line_table" style="border-color:black;font-size:15px">
                        <thead>
                            <tr style="border-color:black">
                                <th name="th_sl_no" class="text-left" style="width:9%"><span>Pos.</span></th>
                                <th name="th_description" class="text-left"><span>Beschreibung</span></th>
                                <th name="th_quantity" class="text-right" style="width:6%"><span>Menge</span></th>
                                <th name="th_priceunit" t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}" style="width:16%"><span>Einzelpreis</span></th>
                                <th name="th_subtotal" class="text-right" style="width:16%">
                                    <span groups="account.group_show_line_subtotals_tax_excluded">Preis in <span t-field="o.currency_id.name"/></span>
                                    <span groups="account.group_show_line_subtotals_tax_included">Preis in <span t-field="o.currency_id.name"/></span>
                                </th>
                            </tr>
                        </thead>
                        <tbody class="invoice_tbody">
                            <t t-set="current_subtotal" t-value="0"/>
                            <t t-set="lines" t-value="o.invoice_line_ids.sorted(key=lambda l: (-l.sequence, l.date, l.move_name, -l.id), reverse=True)"/>

                            <t t-foreach="lines" t-as="line">
                                <t t-set="current_subtotal" t-value="current_subtotal + line.price_subtotal" groups="account.group_show_line_subtotals_tax_excluded"/>
                                <t t-set="current_subtotal" t-value="current_subtotal + line.price_total" groups="account.group_show_line_subtotals_tax_included"/>

                                <tr style="border-color:black;" t-att-class="'bg-200 font-weight-bold o_line_section' if line.display_type == 'line_section' else 'font-italic o_line_note' if line.display_type == 'line_note' else ''">
                                    <t t-if="not line.display_type" name="account_invoice_line_accountable">
                                        <td name="sl_no_line"  style="width:9%;border-color:black;padding-bottom:0px"><span t-field="line.sl_no"/></td>
                                        <td name="account_invoice_line_name" style="border-color:black;padding-bottom:0px">
                                            <t t-if="line.product_id.default_code in ('woo_shipping_fees', 'woo_fees')">
                                                <t t-if="line.shipping_method_title">
                                                     <span t-field="line.shipping_method_title"/>
                                                </t>
                                                <t t-else="">
                                                    <span>Versand: Versandkosten</span>
                                                </t>
                                            </t>
                                            <t t-if="line.product_id.default_code == 'woo_discount'">
                                                <span t-field="line.name" t-options="{'widget': 'text'}"/>
                                            </t>
                                            <t t-if="line.product_id.default_code not in {'woo_shipping_fees','woo_fees','woo_discount'}">
                                                <strong><span t-field="line.product_id.name" t-options="{'widget': 'text'}"/></strong>
                                                <br/>

                                                <span>Produktcode: </span>
                                                <span t-field="line.product_id.default_code" t-options="{'widget': 'text'}"/> &#160;<span class="carret"/>
                                                <t t-set="codes" t-value="o.get_components(line.product_id)"/>
                                                <t t-foreach="codes" t-as="code">
                                                    &#160;<span t-esc="code"/>&#160;<span class="carret"/>
                                                </t>
                                            </t>
                                         </td>
                                        <td class="text-right" style="width:6%;border-color:black;padding-bottom:0px">
                                            <span t-field="line.quantity"/>&#160;<span class="carret"/>
                                        </td>
                                        <td t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}" style="width:16%;border-color:black;padding-bottom:0px">
                                            <span class="text-nowrap" t-field="line.price_unit"/>
                                        </td>
                                        <td class="text-right o_price_total" style="width:16%;border-color:black;padding-bottom:0px">
                                            <span class="text-nowrap" t-esc="'%.2f'% line.price_subtotal" groups="account.group_show_line_subtotals_tax_excluded"/>
                                            <span class="text-nowrap" t-esc="'%.2f'% line.price_total" groups="account.group_show_line_subtotals_tax_included"/>
                                        </td>
                                    </t>
                                </tr>
                            </t>
                        </tbody>
                    </table>

                    <div class="clearfix">
                        <!--<div id="total" class="row">-->
                        <div id="total" class="">
                            <!--<div t-attf-class="#{'col-6' if report_type != 'html' else 'col-sm-7 col-md-6'} ml-auto">-->
                            <div>
                                <table class="table table-sm" style="page-break-inside: avoid;width:100%;">
                                    <!--Tax totals-->
                                    <t t-set="tax_totals" t-value="json.loads(o.tax_totals_json)"/>
                                    <t t-if="o.payment_state == 'paid'">
                                        <t t-call="ourcustom_ever.document_tax_totals_invoice_paid"/>
                                    </t>
                                    <t t-else="">
                                        <t t-call="account.document_tax_totals"/>
                                    </t>
                                </table>
                            </div>
                        </div>
                    </div>

                    <br/><br/>

                    <div name="footer_part">
                         <p><strong style="font-size:14px">Zahlungsbedingungen</strong></p>
                        <hr style="width:100%;border-color:black"/>
                        <p style="font-size:14px">
                            <strong>Herzlichen Dank für Ihren Auftrag.</strong> Bitte bezahlen Sie entweder per beigefügtem <strong>Einzahlungsschein</strong> oder überweisen Sie<br/>
                            den Rechnungsbetrag unter Angabe der <strong>Rechnungsnummer</strong> an: Eversmart products Handels GmbH | IBAN CH92 0900 0000 4149 3985 2
                            <br/>
                            Bitte beachten Sie, dass auf die Bestellung kein Widerrufsrecht besteht.
                        </p>
                    </div>

                </div>

                <!-- ############### QR CODE ##################-->

                <t t-if="o.move_type == 'out_invoice' and o.l10n_ch_currency_name in ['EUR', 'CHF']  and o.payment_state != 'paid' and o.woo_payment_code == 'cheque'">
                    <p style="page-break-after:always;">  </p>
                    <div name="Qr">
                        <style>
                            .swissqr_section_title {
                                font-weight:bold;
                                font-size:16px;
                            }
                            .swissqr_text {
                                font-size:12px;
                            }
                            .title {
                                font-weight:bold;
                            }
                            div#currency_amount {
                                display:inline-block;
                            }
                            .horizontal_scissors {
                                position: absolute;
                                margin-top:-20px;
                                width:20px;
                                height:20px;
                            }
                            .vertical_scissors_bas {
                                position: absolute;
                                width:20px;
                                height:20px;
                                margin-left:-20px;
                                margin-top:90px;
                            }

                        </style>
                        <!-- add class to body tag -->
                        <!--<script>document.body.className += " l10n_ch_qr";</script>-->
                        <!-- add default margin for header (matching A4 European margin) -->
                       <!-- <t t-set="report_header_style">padding-top:6.2mm; padding-left:8.2mm; padding-right:8.2mm;</t>-->

                        <t t-set="formated_amount" t-value="'{:,.2f}'.format(o.amount_residual).replace(',','\xa0')"/>
                        <!--<div class="">
                            <span style="font-size:25px">QR-bill for invoice <t t-esc="o.name"/></span>
                        </div>-->
                        <br/>
                        <div name="empty" style="min-height:380px"></div>
                        <!--####table qr-->
                        <table style="border:2px dotted gray;width:100%">
                            <tr style="border:2px dotted gray;">
                                <td style="border:1px dotted gray;vertical-align:top;width:30%;padding:8px">
                                    <img src="/l10n_ch/static/src/img/scissors_h.png" class="scissors horizontal_scissors"/>
                                    <div class="swissqr_receipt">

                                <!--<img src="/l10n_ch/static/src/img/scissors_h.png" class="scissors horizontal_scissors"/>-->

                                <div id="receipt_title_zone" class="swissqr_section_title">
                                    <span>Receipt</span>
                                </div>
                                  <br/>
                                <div id="receipt_indication_zone" class="receipt_indication_zone">

                                    <div class="swissqr_text title">
                                        <span class="title">Account / Payable to</span><br/>
                                    </div>
                                    <div class="swissqr_text content">
                                        <span t-field="o.partner_bank_id.acc_number" t-if="not o.partner_bank_id.l10n_ch_qr_iban"/>
                                        <span t-field="o.partner_bank_id.l10n_ch_qr_iban" t-if="o.partner_bank_id.l10n_ch_qr_iban"/>
                                        <br/>
                                        <span t-field="o.company_id.name"/><br/>
                                        <span t-field="o.company_id.street"/><br/>
                                        <span t-field="o.company_id.country_id.code"/>
                                        <span t-field="o.company_id.zip"/>
                                        <span t-field="o.company_id.city"/><br/>
                                        <br/>
                                    </div>

                                    <t t-if="o.partner_bank_id.l10n_ch_qr_iban">
                                        <div class="swissqr_text title">
                                            <span class="title">Reference</span>
                                        </div>
                                        <div class="swissqr_text content">
                                            <span t-esc="o.space_qrr_reference(o.payment_reference)"/><br/>
                                            <br/>
                                        </div>
                                    </t>

                                    <div class="swissqr_text title">
                                        <span class="title">Payable by</span>
                                    </div>
                                    <div class="swissqr_text content">
                                        <span t-field="o.partner_id.commercial_partner_id.name"/><br/>
                                        <span t-field="o.partner_id.street"/>
                                        <span t-field="o.partner_id.street2"/><br/>
                                        <span t-field="o.partner_id.country_id.code"/>
                                        <span t-field="o.partner_id.zip"/>
                                        <span t-field="o.partner_id.city"/>
                                    </div>

                                </div>
                                        <br/><br/><br/>
                                <div id="receipt_amount_zone" class="swissqr_column_left receipt_amount_zone">
                                    <div class="swissqr_text">
                                        <div class="column" id="currency_amount">
                                            <div class="title">
                                                <span class="title">Currency</span>
                                            </div>
                                            <div class="content">
                                                <span t-field="o.currency_id.name"/>
                                            </div>
                                        </div>
                                        <div class="column" id="currency_amount" style="padding-left:10px">
                                            <div class="title">
                                                <span class="title">Amount</span><br/>
                                            </div>
                                            <div class="content">
                                                <span t-esc="formated_amount"/>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                        <br/>

                                <div id="receipt_acceptance_point_zone" class="receipt_acceptance_point_zone">
                                    <div class="swissqr_text content" style="float:right">
                                        <span class="title">Acceptance point</span>
                                    </div>
                                </div>
                                        <br/><br/>

                            </div>
                                </td>

                                <td style="border:1px dotted gray;vertical-align:top;width:30%;padding:8px;border-right:hidden">
                                    <div class="swissqr_body">

                                <!--<img src="/l10n_ch/static/src/img/scissors_v.png" class="scissors vertical_scissors"/>-->

                                <div class="swissqr_column_left">

                                    <div class="swissqr_section_title">
                                        <span>Payment part</span>
                                    </div>
                                    <br/>

                                    <!-- //ORIGINAL line
                                    <img class="swissqr" t-att-src="qr_code_urls[o.id]"/>
                                    -->

                                    <img class="swissqr" t-att-src="o.string_data_qr" style="width:180px;height:180px"/>
                                    <!--<img class="swissqr" t-att-src="o.img_qr_path"/>-->
                                  <!--  <img class="swissqr" src="/ourcustom_ever/static/src/img/test.png"/>-->
    <!--
    <img style="max-height: 60px;" alt="Logo" src="data:image/png;base64,"/>
    -->
                                    <br/><br/>
                                    <div id="amount_zone" class="amount_zone">
                                        <div class="swissqr_text">
                                            <div class="column" id="currency_amount">
                                                <div class="title">
                                                    <span class="title">Currency</span>
                                                </div>
                                                <div class="content">
                                                    <span t-field="o.currency_id.name"/>
                                                </div>
                                            </div>
                                            <div class="column" id="currency_amount" style="padding-left:10px">
                                                <div class="title">
                                                    <span class="title">Amount</span><br/>
                                                </div>
                                                <div class="content">
                                                    <span t-esc="formated_amount"/>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <img src="/ourcustom_ever/static/src/img/scissors_vb.png" class="scissors vertical_scissors_bas"/>
                            </div>

                                </td>
                            <td style="border:1px dotted gray;vertical-align:top;width:40%;padding:8px">
                                <div id="indications_zone" class="swissqr_column_right">
                                    <div class="swissqr_text title">
                                        <span class="title">Account / Payable to</span><br/>
                                    </div>
                                    <div class="swissqr_text content">
                                        <span t-field="o.partner_bank_id.acc_number" t-if="not o.partner_bank_id.l10n_ch_qr_iban"/>
                                        <span t-field="o.partner_bank_id.l10n_ch_qr_iban" t-if="o.partner_bank_id.l10n_ch_qr_iban"/>
                                        <br/>
                                        <span t-field="o.company_id.name"/><br/>
                                        <span t-field="o.company_id.street"/><br/>
                                        <span t-field="o.company_id.country_id.code"/>
                                        <span t-field="o.company_id.zip"/>
                                        <span t-field="o.company_id.city"/><br/>
                                        <br/>
                                    </div>

                                    <t t-if="o.partner_bank_id.l10n_ch_qr_iban">
                                        <div class="swissqr_text title">
                                            <span class="title">Reference</span>
                                        </div>
                                        <div class="swissqr_text content">
                                            <span t-esc="o.space_qrr_reference(o.payment_reference)"/><br/>
                                            <br/>
                                        </div>
                                    </t>

                                    <!-- ADDITIONAL INFO
                                    <t t-set="additional_info" t-value="(o.ref or o.name if o.partner_bank_id.l10n_ch_qr_iban else o.payment_reference or o.ref or o.name)"/>
                                    <t t-if="additional_info">
                                        <div class="swissqr_text title">
                                            <span class="title">Additional information</span>
                                        </div>
                                        <div class="swissqr_text content">
                                            <span t-esc="additional_info"/><br/>
                                            <br/>
                                        </div>
                                    </t>-->

                                    <div class="swissqr_text title">
                                        <span class="title">Payable by</span>
                                    </div>
                                    <div class="swissqr_text content">
                                        <span t-field="o.partner_id.commercial_partner_id.name"/><br/>
                                        <span t-field="o.partner_id.street"> </span>
                                        <span t-field="o.partner_id.street2"/><br/>
                                        <span t-field="o.partner_id.country_id.code"/>
                                        <span t-field="o.partner_id.zip"/>
                                        <span t-field="o.partner_id.city"/><br/>
                                        <br/>
                                    </div>
                                </div>

                            </td>
                            </tr>
                        </table>
                    </div>
                </t>
                   <!-- ############### QR CODE ##################-->

            </div>

        </xpath>
    </template>

    <!--Edit Tax table just to add space at left side and set the font size + showing amount tax included in Total-->
    <template id="document_tax_totals_ever" inherit_id="account.document_tax_totals">
        <tr class="border-black o_subtotal" position="replace">
            <tr class="border-black o_subtotal">
                <td style="width:9%;font-size:15px"><span/></td> <!--Before untaxed amount-->
                <td style="font-size:15px"><!--<strong t-esc="subtotal['name']"/>--><strong>Total</strong></td>
                <td class="text-right" style="font-size:15px">
                    <!-- its a string with currency
                    <span
                        t-att-class="oe_subtotal_footer_separator"
                        t-esc="subtotal['formatted_amount']"
                    />-->
                    <strong>
                        <!--ORG: Untaxed amount-->
                       <!-- <span
                        t-att-class="oe_subtotal_footer_separator"
                        t-esc="'%.2f'% subtotal['amount']"
                        />-->
                        <!--Taxed amount-->
                        <span t-att-class="oe_subtotal_footer_separator"
                              t-esc="'%.2f'% tax_totals['amount_total']"
                        />
                    </strong>
                </td>
            </tr>
        </tr>
        <tr class="border-black o_total" position="replace">
            <tr class="border-black o_total">
                <td style="width:9%;font-size:15px"><span/></td>  <!--Before Total-->
                <td style="font-size:15px"><strong>Betrag inkl. MWST</strong></td>
                <td style="font-size:15px" class="text-right">
                    <!--<span t-esc="tax_totals['formatted_amount_total']"/> its a string with currency-->
                    <strong><span t-esc="'%.2f'% tax_totals['amount_total']"/></strong>
                </td>
            </tr>
            <!--These two tr just to add two line bellow Betrag inkl. MWST-->
            <tr class="border-black o_total">
                <td colspan="3" style="padding:1px"><span/></td>
            </tr>
            <tr class="border-black o_total">
                <td colspan="3" style="padding:1px"><span/></td>
            </tr>
        </tr>
    </template>
      <!--Edit Tax table just to add space at left side and set the font size and border color remove border-top-->
    <template id="tax_groups_totals_ever" inherit_id="account.tax_groups_totals">
       <xpath expr="//t/tr[1]" position="replace">
           <tr>
               <t t-if="len(tax_totals['groups_by_subtotal'][subtotal_to_show]) > 1">
                    <td style="width:9%;font-size:15px;border-color:black;border-top:none"><span/></td>
                    <td style="font-size:15px;border-color:black;border-top:none">
                         <!--Manage Translate of TVA-->
                       <!-- <span t-esc="amount_by_group['tax_group_name']"/>-->
                        <t t-if="o.woo_lang_id.code == 'de_DE'">
                            <span t-esc="amount_by_group['tax_group_name'].replace('TVA', 'MWST')"/>
                        </t>
                        <t t-elif="o.woo_lang_id.code == 'en_US'">
                            <span t-esc="amount_by_group['tax_group_name'].replace('TVA', 'VAT')"/>
                        </t>
                        <t t-else="">
                            <span  t-esc="amount_by_group['tax_group_name']"/>
                        </t>

                        <span class="text-nowrap"> auf <!--on-->
                            <!--<t t-esc="amount_by_group['formatted_tax_group_base_amount']"/> its a string with currency-->
                            <t t-esc="'%.2f'% amount_by_group['tax_group_base_amount']"/>
                        </span>
                    </td>
                    <td style="font-size:15px;border-color:black;border-top:none" class="text-right o_price_total">
                        <!--<span class="text-nowrap" t-esc="amount_by_group['formatted_tax_group_amount']"/> its a string with currency-->
                        <span class="text-nowrap" t-esc="'%.2f'% amount_by_group['tax_group_amount']"/>
                    </td>
               </t>
               <t t-else="">
                    <td style="width:9%;font-size:15px;border-color:black;border-top:none"><span/></td>
                    <td style="font-size:15px;border-color:black;border-top:none">
                                <!--Manage Translate of TVA-->
                       <!-- <span class="text-nowrap" t-esc="amount_by_group['tax_group_name']"/>-->
                        <t t-if="o.woo_lang_id.code == 'de_DE'">
                            <span class="text-nowrap" t-esc="amount_by_group['tax_group_name'].replace('TVA', 'MWST')"/>
                        </t>
                        <t t-elif="o.woo_lang_id.code == 'en_US'">
                            <span class="text-nowrap" t-esc="amount_by_group['tax_group_name'].replace('TVA', 'VAT')"/>
                        </t>
                        <t t-else="">
                            <span class="text-nowrap" t-esc="amount_by_group['tax_group_name']"/>
                        </t>
                    </td>
                    <td style="font-size:15px;border-color:black;border-top:none" class="text-right o_price_total">
                        <!--<span class="text-nowrap" t-esc="amount_by_group['formatted_tax_group_amount']" /> its a string with currency-->
                        <span class="text-nowrap" t-esc="'%.2f'% amount_by_group['tax_group_amount']" />
                    </td>
               </t>
           </tr>
        </xpath>
    </template>
</odoo>
